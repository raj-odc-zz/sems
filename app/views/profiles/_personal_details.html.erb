<div class="form-group">
  <div class="col-lg-12 col-md-12 col-sm-12 editable-wrapper">
    <%= f.label :guardian_email_id, :class => "col-lg-3 col-md-3 col-sm-3 control-label" %>
    <div class="col-lg-7 col-md-7 col-sm-7">
      <%= f.text_field :guardian_email_id, :class => "form-control" %>
    </div>
  </div>
</div>
<div class="form-group">
  <div class="col-lg-12 col-md-12 col-sm-12 editable-wrapper">
    <%= f.label :father_name, :class => "col-lg-3 col-md-3 col-sm-3 control-label" %>
    <div class="col-lg-7 col-md-7 col-sm-7">
      <%= f.text_field :father_name, :class => "form-control" %>
    </div>
  </div>
</div>
<div class="form-group">
  <div class="col-lg-12 col-md-12 col-sm-12 editable-wrapper">
    <%= f.label :mother_name, :class => "col-lg-3 col-md-3 col-sm-3 control-label" %>
    <div class="col-lg-7 col-md-7 col-sm-7">
      <%= f.text_field :mother_name, :class => "form-control" %>
    </div>
  </div>
</div>
<div class="form-group">
  <div class="col-lg-12 col-md-12 col-sm-12 editable-wrapper">
    <%= f.label :marital_status, :class => "col-lg-3 col-md-3 col-sm-3 control-label" %>
    <div class="col-lg-3 col-md-3 col-sm-3">
      <%= f.select :marital_status, options_for_select([["Single","single"],["Married","married"]]),{}, :class => "form-control" %>
    </div>
  </div>
</div>
<div class="form-group">
  <div class="col-lg-12 col-md-12 col-sm-12 editable-wrapper">
    <%= f.label :emergency_contact_number, :class => "col-lg-3 col-md-3 col-sm-3 control-label" %>
    <div class="col-lg-7 col-md-7 col-sm-7">
      <%= f.text_field :emergency_contact_number, :class => "form-control mobile_number" %>
    </div>
  </div>
</div>
<div class="form-group">
  <div class="col-lg-12 col-md-12 col-sm-12 editable-wrapper">
    <%= f.label :husband_name, :class => "col-lg-3 col-md-3 col-sm-3 control-label" %>
    <div class="col-lg-7 col-md-7 col-sm-7">
      <%= f.text_field :husband_name, :class => "form-control" %>
    </div>
  </div>
</div>