<% content_for :head do %>
  <%= javascript_include_tag "class_info" %>
<% end %>

<section class="main-content">
  <div class="container">
    <div class="clearfix">
    </div>
    <br />
    <div class="row">
      <a data-toggle="modal" href="#" clas="new-icon" onclick="openModal('/class_lists/new','#classInfoModal')">New</a>
    </div>
    <div class = "row">
      <div class="col-lg-10 col-md-10 col-sm-10">
        <div id="classInfo"></div>
      </div>
    </div>
  </div>
</section>

<!-- Modal -->
<div class="modal fade" id="classInfoModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content"></div>
  </div>
</div>

