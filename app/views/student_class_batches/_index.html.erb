<% content_for :head do %>
    <%= javascript_include_tag "student_batch_class" %>
<% end %>

<section class="main-content">
  <div class="container">
    <div class="clearfix">
    </div>
    <br />
    <div class="row">
      <a data-toggle="modal" href="#" clas="new-icon" onclick="openModal('/student_class_batches/new?profile_id='+<%=profile.id%>,'#batchclassModal')">Add</a>
    </div>
    <div class = "row">
      <div class="col-lg-10 col-md-10 col-sm-10">
        <div id='student_batch_info'></div>
      </div>
    </div>
  </div>
</section>


<!-- Modal -->
<div class="modal fade" id="batchclassModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content"></div>
  </div>
</div>
