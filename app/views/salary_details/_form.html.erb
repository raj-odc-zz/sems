<%= form_for [:api, salary_detail], :remote=>true,:html => {:id =>'salary_detail', :multipart => true, :class => "form-horizontal" } do |f| %>

      <div class="modal-header  clearfix">
        <h4 class="modal-h3" id="myModalLabel">SalaryDetails</h4>
      </div>
      <div class="modal-body">
        <div class="row">
          <div class="form-group" id="staff_list">
              <div class="col-lg-12 col-md-12 col-sm-12 editable-wrapper">
                <%= f.label :staff_id, :class => "col-lg-3 col-md-3 col-sm-3 control-label" %>
                <div class="col-lg-7 col-md-7 col-sm-7">
                  <%= f.select :profile_id, options_from_collection_for_select(Staff.all.nil? ? [] : Staff.all.to_a, "id", "name",f.object.try(:profile_id)),{}, :class => "form-control" %>
                </div>
              </div>
          </div>
           <div class="form-group" id="fees_list">
              <div class="col-lg-12 col-md-12 col-sm-12 editable-wrapper">
                <%= f.label :fees_type_id, :class => "col-lg-3 col-md-3 col-sm-3 control-label" %>
                <div class="col-lg-7 col-md-7 col-sm-7">
                  <%= f.select :fees_type_id, options_from_collection_for_select(FeesType.all.nil? ? [] :       FeesType.all.to_a, "id", "name",f.object.try(:fees_type_id)),{}, :class => "form-control" %>
                </div>
              </div>
          </div>
          <div class="form-group">
            <div class="col-lg-12 col-md-12 col-sm-12 editable-wrapper">
              <%= f.label :amount, "Amount", :class => "col-lg-3 col-md-3 col-sm-3 control-label" %>
              <div class="col-lg-7 col-md-7 col-sm-7">
                <%= f.text_field :amount, :class => "form-control" %>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
        <button type="button" class="btn btn-primary" onclick="submitForm('#salary_detail','#salary_detail_List')">Save</button>
      </div>

<%end%>

