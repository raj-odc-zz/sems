<%= form_for [:api, profile_type], :remote=>true,:html => {:id =>'profile_type', :multipart => true, :class => "form-horizontal" } do |f| %>

      <div class="modal-header  clearfix">
        <h4 class="modal-h3" id="myModalLabel">ProfileType</h4>
      </div>
      <div class="modal-body">
        <div class="row">
          <div class="form-group">
            <div class="col-lg-12 col-md-12 col-sm-12 editable-wrapper">
              <%= f.label :name, "ProfileType", :class => "col-lg-3 col-md-3 col-sm-3 control-label" %>
              <div class="col-lg-7 col-md-7 col-sm-7">
                <%= f.text_field :name, :class => "form-control" %>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
        <button type="button" class="btn btn-primary" onclick="submitForm('#profile_type','#profile_type_List')">Save</button>
      </div>

<%end%>

